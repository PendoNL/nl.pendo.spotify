{
  "id": "nl.pendo.spotify",
  "version": "1.3.1",
  "sdk": 3,
  "brandColor": "#1DB954",
  "compatibility": ">=12.3.0",
  "name": {
    "en": "Spotify Connect"
  },
  "description": {
    "en": "Full control over your audio experience",
    "nl": "Neem de controle over je audio ervaring"
  },
  "author": {
    "name": "Joshua de Gier",
    "email": "joshua@pendo.nl"
  },
  "category": "music",
  "images": {
    "large": "/assets/images/large.jpg",
    "small": "/assets/images/small.jpg"
  },
  "homeyCommunityTopicId": 118309,
  "source": "https://github.com/PendoNL/nl.pendo.spotify",
  "issues": "https://github.com/PendoNL/nl.pendo.spotify/issues",
  "homepage": "https://pendo.nl",
  "bugs": {
    "url": "https://github.com/PendoNL/nl.pendo.spotify/issues"
  },
  "api": {
    "getDiscoveredDevices": {
      "method": "GET",
      "path": "/discovered-devices"
    },
    "startPairing": {
      "method": "POST",
      "path": "/start-pairing"
    },
    "stopPairing": {
      "method": "POST",
      "path": "/stop-pairing"
    },
    "hasCredentials": {
      "method": "GET",
      "path": "/has-credentials"
    },
    "wakeDevice": {
      "method": "POST",
      "path": "/wake-device"
    },
    "resetIdentity": {
      "method": "POST",
      "path": "/reset-identity"
    },
    "wakeDeviceByHost": {
      "method": "POST",
      "path": "/wake-device-by-host"
    }
  }
}